
@{
    ViewData["Title"] = "OtherUsers";
    Layout = "~/Views/Shared/_Admin_Layout.cshtml";
}


    <div class="container">
        <div class="row">
            <div class="col-sm-3">
                <section id="profimd">
                    <img src="~/images/fBE7wrcL_400x400.jpg" class="img-thumbnail" width="200" height="200" />
                    <br />
                </section>

                <section id="info">
                    <h4>Name of the User</h4>
                    <br />
                    <label>Gender</label>
                    <br />
                    <label>BirthDate</label>
                    <br />
                    <p>Bio</p>
                </section>
            </div>

            <div class="col-sm-9">
                @Html.Partial("Post")
            </div>
        </div>
    </div>


    <div class="modal fade" id="ModalLikes" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLongTitle">Likes</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <ul>
                        <li><a href="#" class="as">User 1</a></li>
                        <li><a href="#" class="as">User 2</a></li>
                        <li><a href="#" class="as">User 3</a></li>
                    </ul>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default btns" data-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>

